<?xml version="1.0" encoding="UTF-8"?>
<list>
	<file pattern="Homunculus2" isSafePackage="true">
		<node name="data" reader="UINT" />
		<for name="blessoption" size="#data" hidden="false">
			<node name="group_id" reader="UINT" />
			<wrapper name="type_group">
				<node name="weapon_type" reader="UCHAR" enumName="grade_type" />
				<node name="grade" reader="UCHAR" enumName="item_weapon_v2_type" />
			</wrapper>
			<node name="not_in_use" reader="USHORT" default="0" />
			<node name="effect" reader="CNTR" />
            <for name="effect" size="#effect">
                <node name="OptionType" reader="UINT" enumName="blessoption_type" />
                <node name="EnchantValue" reader="UINT" />
                <node name="optiondesc_skillID" reader="UINT" />
                <node name="optiondesc_skillLevel" reader="UINT" />
            </for>
		</for>
	</file>
	<file pattern="MasterClass3" isSafePackage="true">
		<node name="data_count" reader="UINT" />
		<for name="blessoption" size="#data_count" hidden="false">
			<write name="\r\n" />
			<node name="item_type" reader="UINT" enumName="bless_item_type" />
			<node name="grade" reader="UCHAR" enumName="grade_type" />
			<node name="unk_1" reader="UCHAR" />
			<node name="bodypart" reader="UCHAR" enumName="bodypart_v2_type" />
			<node name="wpn_type" reader="UCHAR" enumName="item_weapon_v2_type" />
			<write name="\r\n" />
			<node name="effect_count" reader="CNTR" />
            <for name="effect" size="#effect_count">
                <node name="OptionType" reader="UINT" enumName="blessoption_type" />
                <node name="EnchantValue" reader="UINT" />
                <node name="optiondesc_skillID" reader="UINT" />
                <node name="optiondesc_skillLevel" reader="UINT" />
            </for>
			<write name="\r\n" />
		</for>
	</file>
	<file pattern="RoseVain" isSafePackage="true">
		<node name="data" reader="UINT" />
		<for name="blessoption" size="#data" hidden="false">
			<node name="group_id" reader="UINT" />
			<wrapper name="type_group">
				<node name="grade" reader="UCHAR" />
				<node name="armor_type" reader="UCHAR" />
				<node name="body_part" reader="UCHAR" />
				<node name="weapon_type" reader="UCHAR" />
			</wrapper>

			<node name="repeat_type" reader="UCHAR" />

			<node name="base_effect" reader="CNTR" />
			<for name="base_effect" size="#base_effect">
				<node name="skillId" reader="UINT" />
				<node name="skillLevel" reader="UINT" />
				<node name="enchant_level" reader="UINT" />
			</for>

			<node name="enchant_effect" reader="CNTR" />
			<for name="enchant_effect" size="#enchant_effect">
				<node name="skillId" reader="UINT" />
				<node name="skillLevel" reader="UINT" />
				<node name="enchant_level" reader="UINT" />
			</for>

			<node name="random_change_look" reader="CNTR" />
			<for name="random_change_look" size="#random_change_look">
				<node name="itemId" reader="UINT" />
				<node name="bless_level" reader="UINT" />
				<node name="enchant_level" reader="UINT" />
			</for>

		</for>
	</file>
	<file pattern="SamuraiCrow" isSafePackage="true">
		<node name="data" reader="UINT" />
		<for name="blessoption" size="#data" hidden="false">
			<node name="group_id" reader="UINT" />
			<wrapper name="type_group">
				<node name="grade" reader="UCHAR" />
				<node name="armor_type" reader="UCHAR" />
				<node name="body_part" reader="UCHAR" />
				<node name="weapon_type" reader="UCHAR" />
			</wrapper>
			<node name="repeat_type" reader="UCHAR" />
			<node name="base_effect" reader="CNTR" />
			<for name="base_effect" size="#base_effect">
				<node name="base_skillId" reader="UINT" />
				<node name="base_skillLevel" reader="UINT" />
				<node name="base_enchant_level" reader="UINT" />
			</for>
			<node name="enchant_effect" reader="CNTR" />
			<for name="enchant_effect" size="#enchant_effect">
				<node name="enchant_skillId" reader="UINT" />
				<node name="enchant_skillLevel" reader="UINT" />
				<node name="enchant_level" reader="UINT" />
			</for>
			<node name="random_change_look" reader="CNTR" />
			<for name="random_change_look" size="#random_change_look">
				<node name="itemId" reader="UINT" />
				<node name="bless_level" reader="UINT" />
				<node name="look_enchant_level" reader="UINT" />
			</for>
		</for>
	</file>
</list>