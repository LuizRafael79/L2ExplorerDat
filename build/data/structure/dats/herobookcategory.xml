<?xml version="1.0" encoding="UTF-8"?>
<list>
<file pattern="SamuraiCrow" isSafePackage="true">
	<node name="group_count" reader="UINT" />
	
	<for name="hero_book_category" size="#group_count" hidden="false">  
		<node name="group_id" reader="UBYTE" />
		<write name="\r\n" />	
		
		<node name="record_count" reader="UBYTE" />
		
		<for name="records" size="#record_count" hidden="false">
			<node name="item" reader="INT" />
			<node name="category" reader="UBYTE" />
		</for>
	</for>
</file>
</list>
