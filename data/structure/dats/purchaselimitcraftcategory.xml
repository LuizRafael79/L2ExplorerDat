<?xml version="1.0" encoding="UTF-8"?>
<list>
	<file pattern="Shinemaker" isSafePackage="true">
		<node name="data" reader="CNTR" />
		<for name="PurchaseLimitCraftCategory" size="#data" hidden="false">
			<node name="category_id" reader="UBYTE" />
			<node name="string_id" reader="UINT" />
			<node name="string_color" reader="RGBA" />
			<node name="ribbon_texture" reader="CNTR" />
			<for name="ribbon_texture" size="#ribbon_texture">
				<node name="ribbon_texture1" reader="MAP_INT" />
			</for>
			<node name="optional" reader="UBYTE" />
		</for>
	</file>
	<file pattern="PathOfRogue" isSafePackage="true">
		<node name="data" reader="CNTR" />
		<for name="PurchaseLimitCraftCategory" size="#data" hidden="false">
			<node name="category_id" reader="UBYTE" />
			<node name="string_id" reader="UINT" />
			<node name="string_color" reader="RGBA" />
			<node name="ribbon_texture" reader="CNTR" />
			<for name="ribbon_texture" size="#ribbon_texture">
				<node name="ribbon_texture1" reader="MAP_INT" />
			</for>
			<node name="optional" reader="UBYTE" />
			<node name="show_server" reader="UCHAR" />
			<for name="show_server" size="#show_server">
				<node name="show_server1" reader="UCHAR" />
			</for>
		</for>
	</file>
	<file pattern="OrcVillage" isSafePackage="true">
	    <node name="data" reader="CNTR" />
	    <for name="lcoin_shop_category" size="#data" hidden="false">
	        <node name="id" reader="UBYTE" />
	        <node name="string_id" reader="UINT" />
	        <node name="string_color" reader="RGBA" />
	        <node name="ribbon_texture" reader="CNTR" />
	        <for name="ribbon_texture" size="#ribbon_texture">
	            <node name="ribbon_texture1" reader="MAP_INT" />
	        </for>
	        <!-- CAMPOS EXTRAS APÃ“S RIBBON_TEXTURE -->
	        <node name="unknown_data1" reader="UINT" />
	        <node name="unknown_data2" reader="UINT" />
	        
	        <node name="optional" reader="UCHAR" />
	        <node name="show_server_count" reader="UCHAR" />
	        <for name="show_server" size="#show_server_count">
	            <node name="server_id" reader="UCHAR" />
	        </for>
	    </for>
	</file>
    <file pattern="SamuraiCrow" isSafePackage="true">
        <node name="PurchaseLimitCraftCategory" reader="UBYTE"/>
        <for name="PurchaseLimitCraftCategory" hidden="false">
            <node name="category_id" reader="UBYTE"/>
            <node name="string_id" reader="UINT"/>
            <node name="string_color" reader="ASCF"/>
            <node name="ribbon_texture" reader="MAP_INT"/>
            <node name="optional" reader="UBYTE"/>
            <node name="show_server" reader="UBYTE"/>
            <for name="show_server" hidden="true">
                <node name="server" reader="UINT"/>
            </for>
			<node name="craftable_list" reader="UBYTE"/>
			<node name="main_string_id" reader="UINT"/>
        </for>
    </file>
</list>
