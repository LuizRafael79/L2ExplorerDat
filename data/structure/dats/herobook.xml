<?xml version="1.0" encoding="UTF-8"?>
<list>
	<file pattern="MasterClass3" isSafePackage="true">
		<node name="data" reader="UINT" />
		<for name="hero_book" size="#data" hidden="false">
			<node name="Id" reader="USHORT" />
			<wrapper name="BookSkill">
				<node name="BookSkillId" reader="UINT" />
				<node name="BookSkillLevel" reader="UBYTE" />
			</wrapper>
			<wrapper name="SuccessItem">
				<node name="SuccessItemId" reader="UINT" />
				<node name="SuccessItemLevel" reader="UINT" />
			</wrapper>
			<wrapper name="SuccessSkill">
				<node name="SuccessSkillId" reader="UINT" />
				<node name="SuccessSkillLevel" reader="UBYTE" />
			</wrapper>
			<node name="MaxPoint" reader="UINT" />
			<node name="Commission" reader="UINT" />
		</for>
	</file>
	<file pattern="AgeOfMagic" isSafePackage="true">
		<node name="data" reader="UINT" />
		<for name="hero_book" size="#data" hidden="false">
			<node name="Id" reader="USHORT" />
			<wrapper name="BookSkill">
				<node name="BookSkillId" reader="USHORT" />
				<node name="BookSkillLevel" reader="UINT" />
			</wrapper>
			<wrapper name="SuccessItem">
				<node name="SuccessItemId" reader="UINT" />
				<node name="SuccessItemLevel" reader="UINT" />
			</wrapper>
			<wrapper name="SuccessSkill">
				<node name="SuccessSkillId" reader="UINT" />
				<node name="SuccessSkillLevel" reader="USHORT" />
			</wrapper>
			<node name="MaxPoint" reader="UINT" />
			<node name="Commission" reader="UINT" />
		</for>
	</file>
	<file pattern="ShieldOfKingdom" isSafePackage="true">
		<node name="data" reader="UINT" />
		<for name="hero_book" size="#data" hidden="false">
			<node name="Id" reader="USHORT" />
			<wrapper name="BookSkill">
				<node name="BookSkillId" reader="USHORT" />
				<node name="BookSkillLevel" reader="UINT" />
			</wrapper>
			<wrapper name="SuccessItem">
				<node name="SuccessItemId" reader="UINT" />
				<node name="SuccessItemLevel" reader="UINT" />
			</wrapper>
			<wrapper name="SuccessSkill">
				<node name="SuccessSkillId" reader="UINT" />
				<node name="SuccessSkillLevel" reader="USHORT" />
			</wrapper>
			<!-- <node name="MaxPoint" reader="UINT" /> -->
			<node name="Commission" reader="UINT" />
		</for>
	</file>
	<file pattern="SamuraiCrow" isSafePackage="true">
		<node name="group_count" reader="UINT" />
	  <for name="hero_book" size="#group_count" hidden="false">  
		<node name="category_id" reader="UBYTE"  />
		<write name="\r\n" />	
		<node name="group_count" reader="UINT" />
			<for name="hero_group" size="#group_count" hidden="false">
				<node name="Id" reader="USHORT" />
				<wrapper name="BookSkill">
					<node name="BookSkillId" reader="USHORT" />
					<node name="BookSkillLevel" reader="UINT" />
				</wrapper>
				<wrapper name="SuccessItem">
					<node name="SuccessItemId" reader="INT" />
					<node name="SuccessItemLevel" reader="INT" />
				</wrapper>
				<wrapper name="SuccessSkill">
					<node name="SuccessSkillId" reader="INT" />
					<node name="SuccessSkillLevel" reader="USHORT" />
				</wrapper>
				<node name="Commission" reader="UINT" />
			</for>
		</for>
	</file>
</list>
